{
    "header": "Melody is an [open source](https://github.com/AllyTally/Melody) Discord bot written by [**Ally \\ðŸŒ #1540**](https://ally.moe/).",
    "footer": "For more information about a command, you can use `help <command_name>`.\nIf you like the bot and would like to support the creator, you can donate to her on [Patreon](https://patreon.com/AllyTally). All donations go towards hosting of the bot (which is expensive!), so it'd be greatly appreciated.",
    "categories": {
        "general": {
            "name": "General",
            "commands": {
                "help": {
                    "description": "Lists the bot's commands.",
                    "usage": "[command]",
                    "example": "hello"
                },
                "hello": {
                    "description": "Returns a wave emoji."
                },
                "calc": {
                    "description": "Executes a math expression.",
                    "usage": "<expression>",
                    "example": "1 + 2"
                },
                "pronouns": {
                    "description": "Lists your or someone else's pronouns.",
                    "usage": "[user]",
                    "example": "Ally"
                },
                "setpronouns": {
                    "description": "Sets the pronouns the bot will use for you."
                },
                "timezone": {
                    "description": "Lists your or someone else's timezone.",
                    "usage": "[user]",
                    "example": "Ally"
                },
                "tf": {
                    "description": "Returns the current time for you or someone else.",
                    "usage": "[user]",
                    "example": "Ally"
                },
                "remind": {
                    "description": "Sets a reminder.",
                    "usage": "<time> <message>",
                    "example": "3h do something"
                },
                "reminders": {
                    "description": "Lists your active reminders."
                },
                "cancelreminder": {
                    "description": "Cancels an active reminder.",
                    "usage": "<reminder_id>",
                    "example": "817122851998924800"
                },
                "int": {
                    "description": "Describes the usage of an [internal command](https://vsix.dev/wiki/Internal_scripting) for the game VVVVVV.",
                    "usage": "<command>",
                    "example": "createcrewman"
                },
                "vtext": {
                    "description": "Generates a textbox from the game VVVVVV.",
                    "usage": "[color] <text>",
                    "example": "cyan Hello!"
                },
                "vtscr": {
                    "description": "Generates a textbox and script from the game VVVVVV.",
                    "usage": "[color] <text>",
                    "example": "cyan Hello!"
                }
            }
        },
        "mod": {
            "name": "Moderator",
            "commands": {
                "warn": {
                    "description": "Gives a user a warning and mutes them depending on how many warning points they have. The amount of points the warning gives can be specified.",
                    "usage": "<user> [--points, -p points] [--no-mute, -nm]",
                    "example": "Ally --points 1 Posting memes in general -nm"
                },
                "mute": {
                    "description": "Gives the user the \"Muted\" role for a period of time, by default forever.",
                    "usage": "<user> [--time, -t time]",
                    "example": "Ally --time 2h You need to take a break"
                }
            }
        },
        "owner": {
            "name": "Owner",
            "commands": {
                "addprefix": {
                    "description": "Adds a global prefix.",
                    "usage": "<prefix>",
                    "example": "m!"
                },
                "removeprefix": {
                    "description": "Removes a global prefix.",
                    "usage": "<prefix>",
                    "example": "m!"
                },
                "clean": {
                    "description": "Deletes the last messages from the bot."
                },
                "eval": {
                    "description": "Evaluates Python code.",
                    "usage": "<code>",
                    "example": "print(\"hi!\")"
                },
                "traceback": {
                    "description": "Returns the last error message."
                },
                "restart": {
                    "description": "Restarts the bot."
                },
                "kill": {
                    "description": "Stops the bot."
                }
            }
        }
    }
}